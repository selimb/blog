    "version": "0.2.0",
    "configurations": [
        {
            "name": "sphinx-build",
            "type": "python",
            "request": "launch",
            "module": "sphinx",
            "args": [
                // sourcedir
                "${workspaceFolder}/source",
                // outputdir
                "${workspaceFolder}/build",
                // everything
                "-a"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
    ]
}